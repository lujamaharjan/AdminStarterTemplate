        console.log("Hell")
        var qs = decodeURIComponent(location.href);
        $('a[href="' + qs + '"]').parents('li').addClass('active');
        $('a[href="' + qs + '"]').parents('li').addClass('toggled');
    </script>*@
</body>

</html>
